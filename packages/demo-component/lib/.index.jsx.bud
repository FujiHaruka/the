'use strict'

const { dir } = require('@the-/templates')
const pkg = require('../package')

module.exports = ['index.jsx'].map((filename) => ({
  ...dir({
    description: pkg.description,
    dirname: __dirname,
    name: pkg.name,
  }),
  path: `${__dirname}/${filename}`,
}))

if (!module.parent) {
  require('coz').render(__filename)
}
