{
  "env": {
    "es6": true,
    "node": true
  },
  "parserOptions": {
    "ecmaFeatures": {
      "jsx": true
    },
    "ecmaVersion": 2018,
    "sourceType": "module"
  },
  "plugins": [],
  "rules": {
    "arrow-body-style": "error",
    "dot-notation": "error",
    "no-dupe-keys": "error",
    "no-duplicate-imports": "error",
    "no-empty-pattern": "error",
    "no-func-assign": "error",
    "no-sparse-arrays": "error",
    "no-var": "error",
    "prefer-const": "error",
    "prefer-destructuring": [
      "error",
      {
        "VariableDeclarator": {
          "array": true,
          "object": true
        },
        "AssignmentExpression": {
          "array": false,
          "object": false
        }
      },
      {
        "enforceForRenamedProperties": false
      }
    ],
    "use-isnan": "error"
  }
}
